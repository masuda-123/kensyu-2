<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.AnswerMapper">
    <select id="getAnswer" resultType="com.example.demo.model.Answer">
        SELECT * FROM correct_answers;
    </select>
    <select id="findById" resultType="com.example.demo.model.Answer">
        SELECT * FROM correct_answers WHERE questions_id = #{questionId};
    </select>
</mapper>